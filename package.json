{
  "name": "@chewhx/google-books",
  "version": "0.1.0",
  "description": "Search for books on Google Books",
  "main": "./dist/index.js",
  "scripts": {
    "watch:tsc": "tsc --watch",
    "build:tsc": "tsc",
    "rebuild:tsc": "rm -rf dist && npm run build:tsc",
    "watch:app": "nodemon ./dist/index.js",
    "dev": "concurrently \"npm run watch:tsc\" && \"npm run watch:app\""
  },
  "keywords": [
    "google",
    "books",
    "search"
  ],
  "author": "Chew Han Xiang",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "repository": {
    "url": "https://github.com/chewhx/google-books",
    "directory": "https://github.com/chewhx/google-books"
  }
}
